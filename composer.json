{
    "name": "reinelt/lumineer",
    "description": "This package provides a flexible way to add Role-based Permissions to Lumen",
    "keywords": ["laravel","lumen","illuminate","auth","roles","acl", "rbac","permission"],
    "license": "MIT",
     "require": {
        "php": ">=5.5.0",
        "illuminate/console": "~5.0",
        "illuminate/support": "~5.0",
        "illuminate/cache": "~5.0",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "mockery/mockery": "dev-master",
        "illuminate/database": "~5.0",
        "sami/sami": "dev-master",
        "squizlabs/php_codesniffer": "2.*"
    },
    "autoload": {
        "classmap": [
            "src/commands"
        ],
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Reinelt\\Lumineer\\": "src/Lumineer/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/Middleware/MiddlewareTest.php"
        ]
    },
    "minimum-stability": "dev"
}
